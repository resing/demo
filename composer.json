{
    "name": "symfony/website-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A skeleton to start a new Symfony website",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-migrations-bundle": "^1.3|^2.0",
        "doctrine/orm": "^2.5.11",
        "phpdocumentor/reflection-docblock": "^3.0|^4.0",
        "phpmd/phpmd": "^2.8",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "4.0.*",
        "symfony/cache": "4.0.*",
        "symfony/config": "4.0.*",
        "symfony/console": "4.0.*",
        "symfony/dependency-injection": "4.0.*",
        "symfony/doctrine-bridge": "4.0.*",
        "symfony/event-dispatcher": "4.0.*",
        "symfony/expression-language": "4.0.*",
        "symfony/filesystem": "4.0.*",
        "symfony/finder": "4.0.*",
        "symfony/flex": "^1.0",
        "symfony/form": "4.0.*",
        "symfony/framework-bundle": "4.0.*",
        "symfony/http-foundation": "4.0.*",
        "symfony/http-kernel": "4.0.*",
        "symfony/inflector": "4.0.*",
        "symfony/intl": "4.0.*",
        "symfony/monolog-bridge": "4.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/options-resolver": "4.0.*",
        "symfony/process": "4.0.*",
        "symfony/profiler-pack": "^1.0",
        "symfony/property-access": "4.0.*",
        "symfony/property-info": "4.0.*",
        "symfony/routing": "4.0.*",
        "symfony/security": "4.0.*",
        "symfony/security-bundle": "4.0.*",
        "symfony/serializer": "4.0.*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.0.*",
        "symfony/twig-bundle": "4.0.*",
        "symfony/validator": "4.0.*",
        "symfony/web-link": "4.0.*",
        "symfony/yaml": "4.0.*"
    },
    "require-dev": {
        "easycorp/easy-log-handler": "^1.0.7",
        "symfony/browser-kit": "^3.3|^4.0",
        "symfony/css-selector": "^3.3|^4.0",
        "symfony/debug": "4.0.*",
        "symfony/debug-bundle": "4.0.*",
        "symfony/dom-crawler": "4.0.*",
        "symfony/dotenv": "4.0.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "4.1.*",
        "symfony/stopwatch": "4.0.*",
        "symfony/var-dumper": "4.0.*",
        "symfony/web-profiler-bundle": "4.0.*",
        "symfony/web-server-bundle": "4.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "4.0.*",
        "symfony/polyfill-php71": "4.0.*",
        "symfony/polyfill-php70": "4.0.*",
        "symfony/polyfill-php56": "4.0.*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "4.0.*"
    },
    "extra": {
        "symfony": {
            "require": "4.0.*",
            "allow-contrib": false
        }
    }
}
